<div class="user-main__about">
  <% if user.profile_image.attached? %>
    <%= image_tag user.profile_image.variant(resize: "100x100"), class: "about_img" %>
  <% else %>
    <%= image_tag "user.png", class: "about_img" %>
  <% end %>
  <div>
    <h1 class="about_name"><%= user.name %></h1>
    <p class="about_desc"><%= user.description %></p>
  </div>
</div>